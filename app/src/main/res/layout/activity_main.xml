        android:layout_height="66dp"
        android:layout_marginTop="88dp"
        android:layout_marginEnd="12dp"
        android:backgroundTint="#C1C1C1"
        android:onClick="bentuenifai"
        android:text="25%"
        android:textColor="#000000"
        app:layout_constraintEnd_toStartOf="@+id/theri"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <Button
        android:id="@+id/theri"
        android:layout_width="66dp"
        android:layout_height="66dp"
        android:layout_marginTop="88dp"
        android:layout_marginEnd="16dp"
        android:onClick="bentheri"
        android:backgroundTint="#C1C1C1"
        android:text="30%"
        android:textColor="#050404"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <Button
        android:id="@+id/ten"
        android:layout_width="66dp"
        android:layout_height="66dp"
        android:layout_marginTop="88dp"
        android:layout_marginEnd="12dp"
        android:backgroundTint="#C1C1C1"
        android:text="10%"
        android:onClick="benten"
        android:textColor="#070505"
        app:layout_constraintEnd_toStartOf="@+id/fiftin"
        app:layout_constraintHorizontal_bias="0.941"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <Button
        android:id="@+id/fiftin"
        android:layout_width="66dp"
        android:layout_height="66dp"
        android:layout_marginTop="88dp"
        android:layout_marginEnd="8dp"
        android:backgroundTint="#C1C1C1"
        android:onClick="benfiftin"
        android:text="15%"
        android:textColor="#050404"
        app:layout_constraintEnd_toStartOf="@+id/tueni"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="76dp"
